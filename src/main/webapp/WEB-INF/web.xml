<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee" id="WebApp_ID">
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>about.jsp</welcome-file>
    <welcome-file>contact.jsp</welcome-file>
    <welcome-file>help.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.tdtu.servlets.LoginServlet</servlet-class>
    <servlet-name>image1</servlet-name>
    <servlet-class>com.tdtu.servlets.Image1Servlet</servlet-class>
    <servlet-name>image2</servlet-name>
    <servlet-class>com.tdtu.servlets.Image2Servlet</servlet-class>
    <servlet-name>download</servlet-name>
    <servlet-class>com.tdtu.servlets.DownloadServlet</servlet-class>
    <servlet-name>product</servlet-name>
    <servlet-class>com.tdtu.servlets.ProductServlet</servlet-class>
    <servlet-name>control</servlet-name>
    <servlet-class>com.tdtu.servlets.ControlServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
    <servlet-name>image1</servlet-name>
    <url-pattern>/image1</url-pattern>
    <servlet-name>image2</servlet-name>
    <url-pattern>/image2</url-pattern>
    <servlet-name>download</servlet-name>
    <url-pattern>/download</url-pattern>
    <servlet-name>product</servlet-name>
    <url-pattern>/product</url-pattern>
    <servlet-name>control</servlet-name>
    <url-pattern>/control</url-pattern>
  </servlet-mapping>
</web-app>
